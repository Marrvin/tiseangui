	<command>	
		<name>ar-model</name>
		<commandLineName>ar-model</commandLineName>
		<description>
			Fits an multivariate AR model to the data and gives the coefficients and the residues (or an iterated model).
		</description>
		
		<parameters>
			<parameter>
				<value>-l</value>
				<name>length of file [default is whole file]</name>
				<type>integer</type>
			</parameter>
			<parameter>
				<value>-x</value>
				<name># of lines to be ignored [default is 0]</name>
				<type>integer</type>
			</parameter>
			<parameter>
				<value>-m</value>
				<name>dimension [default is 1]</name>
				<type>integer</type>
			</parameter>
			<parameter>
				<value>-c</value>
				<name>columns to read [default is 1,...,dimension]</name>
				<type>integer</type>
			</parameter>
			<parameter>
				<value>-p</value>
				<name>#order of AR-Fit [default is 1]</name>
				<type>integer</type>
			</parameter>
			<parameter>
				<value>-V</value>
				<name>verbosity level [default is 1]</name>
				<type>options</type>
				<options>
					<option>0</option>
					<option>1</option>
					<option>2</option>
					<option>4</option>															
				</options>
			</parameter>
		</parameters>		
	</command>

	<command>	
		<name>c2d</name>
		<commandLineName>c2d</commandLineName>
		<description>
			local slopes from c1/c2 correlation sum data.
		</description>
		
		<parameters>
			<parameter required="true">
				<value>-a</value>
				<name>average using -#,...,+#</name>
				<type>integer</type>
			</parameter>
			<parameter required="true">
				<value>-V</value>
				<name>verbosity level (0 = only fatal errors)</name>
				<type>integer</type>
			</parameter>
			
		</parameters>		
	</command>
	
</tiseanCommandConfig>
